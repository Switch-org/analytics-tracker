{
  "name": "user-analytics-tracker",
  "version": "2.1.0",
  "description": "Comprehensive analytics tracking library with device detection, network analysis, location tracking, and IP geolocation",
  "keywords": [
    "analytics",
    "tracking",
    "device-detection",
    "network-detection",
    "geolocation",
    "user-analytics",
    "ip-geolocation",
    "hotspot-detection"
  ],
  "author": "Switch Org",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/switch-org/analytics-tracker.git"
  },
  "bugs": {
    "url": "https://github.com/switch-org/analytics-tracker/issues"
  },
  "homepage": "https://github.com/switch-org/analytics-tracker#readme",
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.esm.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.esm.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs.js"
      }
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "rollup -c",
    "build:watch": "rollup -c --watch",
    "dev": "npm run build:watch",
    "test": "vitest run",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,json,md}\"",
    "type-check": "tsc --noEmit",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run clean && npm run lint && npm run type-check && npm run test && npm run build",
    "prepack": "npm run clean && npm run build",
    "release": "semantic-release",
    "docs": "typedoc src/index.ts"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": false
    },
    "react-dom": {
      "optional": false
    }
  },
  "dependencies": {},
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.1",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@rollup/plugin-typescript": "^12.1.1",
    "@types/node": "^20.17.10",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^8.11.0",
    "@typescript-eslint/parser": "^8.11.0",
    "@vitest/coverage-v8": "^2.1.8",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.37.3",
    "eslint-plugin-react-hooks": "^5.1.0",
    "prettier": "^3.3.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "jsdom": "^25.0.1",
    "rimraf": "^6.0.1",
    "rollup": "^4.28.1",
    "rollup-plugin-dts": "^6.1.1",
    "semantic-release": "^24.0.0",
    "tslib": "^2.8.1",
    "typedoc": "^0.28.14",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
